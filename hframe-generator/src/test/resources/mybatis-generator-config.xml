            <property name="enableSubPackages" value="false"/>
        </sqlMapGenerator>
        <!-- DAO 瀵硅薄瀹炰綋 -->
        <javaClientGenerator targetPackage="com.hframe.dao" targetProject="hframe-core/src/main/java" type="XMLMAPPER">
